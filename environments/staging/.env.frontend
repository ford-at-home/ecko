# Frontend Environment Configuration - Staging
# This file contains environment-specific settings for the frontend application

# App Configuration
REACT_APP_ENV=staging
REACT_APP_NAME=Echoes
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info

# API Configuration
REACT_APP_API_BASE_URL=https://api-staging.echoes.app
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_ATTEMPTS=3

# AWS Configuration
REACT_APP_AWS_REGION=us-east-1
REACT_APP_AWS_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
REACT_APP_AWS_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
REACT_APP_AWS_COGNITO_IDENTITY_POOL_ID=us-east-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
REACT_APP_AWS_S3_BUCKET=echoes-audio-staging

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_DEBUG_PANEL=false

# Audio Configuration
REACT_APP_MAX_RECORDING_DURATION=30
REACT_APP_AUDIO_QUALITY=high
REACT_APP_SUPPORTED_FORMATS=webm,mp4,wav

# UI Configuration
REACT_APP_THEME=auto
REACT_APP_ANIMATION_SPEED=normal
REACT_APP_ENABLE_HAPTICS=true

# Development Tools
REACT_APP_MOCK_API=false
REACT_APP_SHOW_REDUX_DEVTOOLS=false
REACT_APP_HOT_RELOAD=false

# Security
REACT_APP_CSP_ENABLED=true
REACT_APP_HTTPS_ONLY=true

# Performance
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_CACHE_STRATEGY=cacheFirst
REACT_APP_PRELOAD_CRITICAL_RESOURCES=true

# Analytics & Monitoring
REACT_APP_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X
REACT_APP_MIXPANEL_TOKEN=xxxxxxxxxxxxxxxxxxxxxxxxxx

# Social & Sharing
REACT_APP_ENABLE_SOCIAL_SHARING=true
REACT_APP_DEFAULT_SHARE_MESSAGE=Check out my Echo from Echoes!

# Version & Build Info (Auto-populated by CI/CD)
REACT_APP_VERSION=
REACT_APP_BUILD_TIME=
REACT_APP_GIT_COMMIT=