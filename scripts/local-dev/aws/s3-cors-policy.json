{
  "CORSRules": [
    {
      "ID": "EchoesDevCORS",
      "AllowedHeaders": ["*"],
      "AllowedMethods": ["GET", "PUT", "POST", "DELETE", "HEAD"],
      "AllowedOrigins": [
        "http://localhost:3000",
        "http://127.0.0.1:3000",
        "https://dev.echoes.app",
        "https://mobile-dev.echoes.app"
      ],
      "ExposeHeaders": ["ETag", "x-amz-meta-custom-header"],
      "MaxAgeSeconds": 3000
    }
  ]
}